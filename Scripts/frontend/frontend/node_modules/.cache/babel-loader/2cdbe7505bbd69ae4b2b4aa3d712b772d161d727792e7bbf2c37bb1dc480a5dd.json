  method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify({\n            audio: base64Audio\n          })\n        });\n        if (!response.ok) throw new Error(`Server error: ${response.status}`);\n        const data = await response.json();\n        const targetScore = data.score ?? 0;\n        console.log(\"Detection score:\", targetScore);\n        await this.animateScore(targetScore);\n      } catch (err) {\n        console.error(\"Detection error:\", err);\n        alert(\"Error detecting watermark. Check console for details.\");\n        this.score = 0;\n      } finally {\n        this.detecting = false;\n      }\n    },\n    fileToBase64(file) {\n      return new Promise((resolve, reject) => {\n        const reader = new FileReader();\n        reader.onload = () => resolve(reader.result.split(\",\")[1]);\n        reader.onerror = reject;\n        reader.readAsDataURL(file);\n      });\n    },\n    async animateScore(target) {\n      return new Promise(resolve => {\n        const step = (target - this.score) / 30;\n        let count = 0;\n        const interval = setInterval(() => {\n          this.score = Math.min(100, Math.max(0, this.score + step));\n          count++;\n          if (count >= 30) {\n            this.score = Math.round(target);\n            clearInterval(interval);\n            resolve();\n          }\n        }, 40);\n      });\n    }\n  }\n};","map":{"version":3,"names":["_SidePanel","_interopRequireDefault","require","_default","exports","default","name","components","SidePanel","data","size","stroke","center","radius","circum","Math","PI","gradId","random","toString","slice","rotate","detecting","score","file","computed","ringDash","gap","effective","show","max","min","hide","buttonText","methods","handleFileUpload","event","target","files","toggleDetect","alert","base64Audio","fileToBase64","token","localSto