-else class=\"line2\">{{ score }}%</div>\r\n            </div>\r\n\r\n            <div class=\"button-wrap\">\r\n              <label class=\"upload-btn\">\r\n                <input type=\"file\" accept=\"audio/*\" @change=\"onFileChange\" hidden />\r\n                {{ selectedFile ? selectedFile.name : \"Choose Audio File\" }}\r\n              </label>\r\n\r\n              <button class=\"detect-btn\" :disabled=\"detecting\" @click=\"toggleDetect\">\r\n                {{ buttonText }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidePanel from \"@/components/SidePanel.vue\";\r\n\r\nexport default {\r\n  name: \"DetectionResult\",\r\n  components: { SidePanel },\r\n  data() {\r\n    const size = 466;\r\n    const stroke = 48;\r\n    const center = size / 2;\r\n    const radius = (size - stroke) / 2;\r\n    const circum = 2 * Math.PI * radius;\r\n\r\n    return {\r\n      size,\r\n      stroke,\r\n      center,\r\n      radius,\r\n      circum,\r\n      gradId: \"grad-donut-\" + Math.random().toString(36).slice(2, 8),\r\n      rotate: `rotate(-90 ${center} ${center})`,\r\n      detecting: false,\r\n      score: 0,\r\n      file: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ringDash() {\r\n      const gap = this.circum * 0.06;\r\n      const effective = this.circum - gap;\r\n      const show = Math.max(0, Math.min(1, this.score / 100)) * effective;\r\n      const hide = this.circum - show;\r\n      return `${show} ${hide}`;\r\n    },\r\n    buttonText() {\r\n      return this.detecting ? \"Detecting...\" : \"Detect\";\r\n    },\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.file = file;\r\n      }\r\n    },\r\n\r\n    async toggleDetect() {\r\n      if (this.detecting || !this.file) {\r\n        alert(\"Please select an audio file before detection.\");\r\n        return;\r\n      }\r\n\r\n      this.detecting = true;\r\n      this.score = 0;\r\n\r\n      try {\r\n        const base64Audio = await this.fileToBase64(this.file);\r\n        const token = localStorage.getItem(\"token\");\r\n\r\n        console.log(\"Sending audio to Flask...\");\r\n        const response = await fetch(\"http://127.0.0.1:5000/detect\", {\r\n          method: \"POST\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n          body: JSON.stringify({ audio: base64Audio }),\r\n        });\r\n\r\n        if (!response.ok) throw new Error(`Server error: ${response.status}`);\r\n\r\n        const data = await response.json();\r\n        const targetScore = data.score ?? 0;\r\n        console.log(\"Detection score:\", targetScore);\r\n\r\n        await this.animateScore(targetScore);\r\n      } catch (err) {\r\n        console.error(\"Detection error:\", err);\r\n        alert(\"Error detecting watermark. Check console for details.\");\r\n        this.score = 0;\r\n      } finally {\r\n        this.detecting = false;\r\n      }\r\n    },\r\n\r\n    fileToBase64(file) {\r\n      return new Promise((resolve, reject) => {\r\n        const reader = new FileReader();\r\n        reader.onload = () => resolve(reader.result.split(\",\")[1]);\r\n        reader.onerror = reject;\r\n        reader.readAsDataURL(file);\r\n      });\r\n    },\r\n\r\n    async animateScore(target) {\r\n      return new Promise((resolve) => {\r\n        const step = (target - this.score) / 30;\r\n        let count = 0;\r\n        const interval = setInterval(() => {\r\n          this.score = Math.min(100, Math.max(0, this.score + step));\r\n          count++;\r\n          if (count >= 30) {\r\n            this.score = Math.round(target);\r\n            clearInterval(interval);\r\n            resolve();\r\n          }\r\n        }, 40);\r\n      });\r\n    },\r\n  },\r\n};