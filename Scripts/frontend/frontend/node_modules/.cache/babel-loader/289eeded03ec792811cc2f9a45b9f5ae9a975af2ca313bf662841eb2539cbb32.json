\n.primary-btn:disabled {\r\n  background: #333;\r\n  cursor: not-allowed;\r\n}\r\n\r\n.error {\r\n  color: #ef4444;\r\n  text-align: center;\r\n  margin-top: 10px;\r\n  font-size: 0.9em;\r\n}\r\n\r\n.bottom-row {\r\n  text-align: center;\r\n  margin-top: 15px;\r\n  font-size: 0.9em;\r\n  color: #888;\r\n}\r\n\r\n.bottom-row .link {\r\n  color: #60a5fa;\r\n  text-decoration: none;\r\n  margin-left: 5px;\r\n}\r\n\r\n.bottom-row .link:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;AA4CA,IAAAA,IAAA,GAAAC,sBAAA,CAAAC,OAAA;AAAA,IAAAC,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAC,QAAA;MACA;MACAC,OAAA;MACAC,OAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,OAAA;MACA,KAAAF,KAAA;MACA,KAAAD,OAAA;MACA;QACA,MAAAI,GAAA,SAAAC,YAAA,CAAAC,IAAA;UACAT,KAAA,OAAAD,IAAA,CAAAC,KAAA,CAAAU,IAAA;UACAT,QAAA,OAAAF,IAAA,CAAAE,QAAA,CAAAS,IAAA;QACA;QAEAC,YAAA,CAAAC,OAAA,UAAAL,GAAA,CAAAT,IAAA,CAAAe,KAAA;QACAF,YAAA,CAAAC,OAAA,aAAAL,GAAA,CAAAT,IAAA,CAAAgB,QAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA,SAAAC,GAAA;QACA,KAAAb,KAAA,GAAAa,GAAA,CAAAC,QAAA,EAAApB,IAAA,EAAAqB,OAAA;MACA;QACA,KAAAhB,OAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"script","externalDependencies":[]}