event>\n          <div class=\"field\">\n            <label class=\"label\">Email</label>\n            <div class=\"input-like\">\n              <el-input v-model=\"form.email\" placeholder=\"balamia@gmail.com\" clearable />\n            </div>\n          </div>\n\n          <div class=\"field\">\n            <div class=\"label-row\">\n              <label class=\"label\">Password</label>\n              <router-link class=\"forgot\" to=\"/forgot\">Forgot ?</router-link>\n            </div>\n            <div class=\"input-like\">\n              <el-input\n                :type=\"showPwd ? 'text' : 'password'\"\n                v-model=\"form.password\"\n                placeholder=\"Enter your password\"\n                @keyup.enter.native=\"submit\"\n              >\n                <template #suffix>\n                  <i\n                    :class=\"['el-icon-view', 'eye', { on: showPwd }]\"\n                    @click.stop=\"showPwd = !showPwd\"\n                    title=\"Show / Hide\"\n                  />\n                </template>\n              </el-input>\n            </div>\n          </div>\n\n          <el-button class=\"primary\" type=\"primary\" @click=\"submit\" :loading=\"loading\">\n            Login now\n          </el-button>\n\n          <div class=\"bottom-row\">\n            <span>Don't Have An Account?</span>\n            <router-link class=\"link\" to=\"/signup\">Sign Up</router-link>\n          </div>\n        </el-form>\n\n        <p v-if=\"error\" class=\"error\">{{ error }}</p>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginPage\",\n  data() {\n    return {\n      form: {\n        email: \"\",\n        password: \"\",\n      },\n      showPwd: false,\n      loading: false,\n      error: \"\",\n    };\n  },\n  methods: {\n    async submit() {\n      this.error = \"\";\n      this.loading = true;\n      try {\n        const res = await axios.post(`${process.env.VUE_APP_API_URL}/login`, {\n          email: this.form.email,\n          password: this.form.password,\n        });\n        localStorage.setItem(\"token\", res.data.token);\n        localStorage.setItem(\"username\", res.data.username);\n        this.$router.push(\"/home\");\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Invalid credentials\";\n      } finally {\n        this.loading = false;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n$bg: #000;\n$text: #e6e8eb;\n\n.login {\n  min-height: 100vh;\n  background: $bg;\n  color: $text;\n  position: relative;\n  overflow: hidden;\n}\n\n/* ðŸŸª Match looper position from SignUp.vue */\n.looper {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: url(\"@/assets/looper-bg.png\") center center / cover no-repeat;\n  opacity: 0.4;\n  pointer-events: none;\n}\n\n.form-wrap {\n  display: grid;\n  place-items: center;\n  min-height: 100vh;\n  padding: 24px;\n}\n\n.form-card {\n  width: 426px;\n  background: #0b0b0b; /* solid black like SignUp */\n  border-radius: 20px;\n  box-shadow: 0 8px 40px rgba(0, 0, 0, 0.6);\n  padding: 48px 72px;\n}\n\n.title {\n  margin: 0 0 24px;\n  font: 600 28px/1 \"Poppins\", system-ui;\n  color: #fafafa;\n}\n\n.el-reset {\n  display: flex;\n  flex-direction: column;\n  gap: 24px;\n}\n.field {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n.label {\n  font: 400 16px/1 \"Poppins\", system-ui;\n  color: #d1d1d6;\n}\n.label-row {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n.forgot {\n  color: #a0a0ab;\n  font: 400 16px/1 \"Poppins\", system-ui;\n}\n\n.input-like {\n  border: 3px solid #ebedf0;\n  border-radius: 8px;\n  overflow: hidden;\n\n  ::v-deep(.el-input__inner) {\n    height: 42px;\n    line-height: 42px;\n    background: transparent;\n    border: none;\n    color: #f2f2f7;\n    padding: 0 36px 0 16px;\n  }\n\n  ::v-deep(.el-input__suffix) {\n    right: 10px;\n  }\n\n  .eye {\n    color: #70707b;\n    cursor: pointer;\n    position: relative;\n    top: 25%;\n  }\n  .eye.on {\n    color: #f2f2f7;\n  }\n}\n\n.primary {\n  height: 52px;\n  width: 100%;\n  border-radius: 8px;\n  background: #1570ef;\n  border: none;\n  font: 600 16px/1 \"Poppins\", system-ui;\n  color: #fcfcfc;\n}\n\n.bottom-row {\n  display: flex;\n  justify-content: center;\n  gap: 8px;\n  margin-top: 4px;\n  span {\n    color: #70707b;\n    font: 400 16px/1 \"Poppins\", system-ui;\n  }\n  .link {\n    color: #a0a0ab;\n    font: 400 16px/1 \"Poppins\", system-ui;\n  }\n}\n\n.error {\n  color: #ef4444;\n  margin-top: 15px;\n  text-align: center;\n}\n\n@media (max-width: 720px) {\n  .form-card {\n    width: 100%;\n    padding: 32px 20px;\n  }\n}\n</style>\n"],"mappings":";;;;;;;;AAyDA,IAAAA,MAAA,GAAAC,sBAAA,CAAAC,OAAA;AAAA,IAAAC,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;QACAC,KAAA;QACAC,QAAA;MACA;MACAC,OAAA;MACAC,OAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,OAAA;MACA,KAAAF,KAAA;MACA,KAAAD,OAAA;MACA;QACA,MAAAI,GAAA,SAAAC,cAAA,CAAAC,IAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAC,eAAA;UACAZ,KAAA,OAAAD,IAAA,CAAAC,KAAA;UACAC,QAAA,OAAAF,IAAA,CAAAE;QACA;QACAY,YAAA,CAAAC,OAAA,UAAAP,GAAA,CAAAT,IAAA,CAAAiB,KAAA;QACAF,YAAA,CAAAC,OAAA,aAAAP,GAAA,CAAAT,IAAA,CAAAkB,QAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA,SAAAC,GAAA;QACA,KAAAf,KAAA,GAAAe,GAAA,CAAAC,QAAA,EAAAtB,IAAA,EAAAuB,OAAA;MACA;QACA,KAAAlB,OAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"script","externalDependencies":[]}