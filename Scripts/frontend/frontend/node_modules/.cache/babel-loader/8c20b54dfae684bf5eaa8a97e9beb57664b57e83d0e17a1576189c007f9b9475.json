t>\r\n              </defs>\r\n\r\n              <circle\r\n                :cx=\"center\"\r\n                :cy=\"center\"\r\n                :r=\"radius\"\r\n                :stroke=\"`url(#${gradId})`\"\r\n                :stroke-width=\"stroke\"\r\n                fill=\"none\"\r\n                stroke-linecap=\"butt\"\r\n                :stroke-dasharray=\"ringDash\"\r\n                :transform=\"rotate\"\r\n              />\r\n            </svg>\r\n\r\n            <div class=\"center-text\">\r\n              <div v-if=\"detecting\" class=\"line2 recording\">●</div>\r\n              <div v-else class=\"line2\">{{ score }}%</div>\r\n            </div>\r\n\r\n            <!-- File Upload -->\r\n            <div class=\"upload-wrap\">\r\n              <input\r\n                type=\"file\"\r\n                accept=\"audio/*\"\r\n                @change=\"handleFileUpload\"\r\n                id=\"audioUpload\"\r\n                class=\"file-input\"\r\n              />\r\n              <label for=\"audioUpload\" class=\"upload-label\">Choose Audio File</label>\r\n\r\n              <p v-if=\"fileName\" class=\"file-status\">\r\n                Selected: {{ fileName }}\r\n                <span v-if=\"uploaded\" class=\"uploaded-check\">✔ Uploaded</span>\r\n              </p>\r\n            </div>\r\n\r\n            <!-- Detect Button -->\r\n            <div class=\"button-wrap\">\r\n              <button\r\n                class=\"detect-btn\"\r\n                :disabled=\"detecting\"\r\n                @click=\"toggleDetect\"\r\n              >\r\n                {{ buttonText }}\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidePanel from \"@/components/SidePanel.vue\";\r\n\r\nexport default {\r\n  name: \"DetectionResult\",\r\n  components: { SidePanel },\r\n  data() {\r\n    const size = 466;\r\n    const stroke = 48;\r\n    const center = size / 2;\r\n    const radius = (size - stroke) / 2;\r\n    const circum = 2 * Math.PI * radius;\r\n\r\n    return {\r\n      size,\r\n      stroke,\r\n      center,\r\n      radius,\r\n      circum,\r\n      gradId: \"grad-donut-\" + Math.random().toString(36).slice(2, 8),\r\n      rotate: `rotate(-90 ${center} ${center})`,\r\n      detecting: false,\r\n      score: 0,\r\n      file: null,\r\n      fileName: \"\",\r\n      uploaded: false,\r\n    };\r\n  },\r\n  computed: {\r\n    ringDash() {\r\n      const gap = this.circum * 0.06;\r\n      const effective = this.circum - gap;\r\n      const show = Math.max(0, Math.min(1, this.score / 100)) * effective;\r\n      const hide = this.circum - show;\r\n      return `${show} ${hide}`;\r\n    },\r\n    buttonText() {\r\n      return this.detecting ? \"Detecting...\" : \"Detect\";\r\n    },\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const selected = event.target.files[0];\r\n      if (selected) {\r\n        this.file = selected;\r\n        this.fileName = selected.name;\r\n        this.uploaded = true;\r\n        console.log(\"Selected file:\", this.fileName);\r\n\r\n        // show uploaded indicator for 2 seconds\r\n        setTimeout(() => {\r\n          this.uploaded = false;\r\n        }, 2000);\r\n      }\r\n    },\r\n\r\n    async toggleDetect() {\r\n      if (this.detecting) return;\r\n\r\n      if (!this.file) {\r\n        alert(\"Please select an audio file before detection.\");\r\n        return;\r\n      }\r\n\r\n      this.detecting = true;\r\n      this.score = 0;\r\n\r\n      try {\r\n        const base64Audio = await this.fileToBase64(this.file);\r\n        const token = localStorage.getItem(\"token\");\r\n\r\n        console.log(\"Uploading file:\", this.fileName);\r\n        const response = await fetch(\"http://127.0.0.1:5000/detect\", {\r\n          method: \"POST\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: `Bearer ${token}`,\r\n          },\r\n          body: JSON.stringify({ audio: base64Audio }),\r\n        });\r\n\r\n        if (!response.ok) throw new Error(`Server error: ${response.status}`);\r\n        const data = await response.json();\r\n\r\n        const targetScore = data.score ?? 0;\r\n        console.log(\"Detection score:\", targetScore);\r\n        await this.animateScore(targetScore);\r\n      } catch (err) {\r\n        console.error(\"Detection error:\", err);\r\n        alert(\"Error detecting watermark. Check console for details.\");\r\n        this.score = 0;\r\n      } finally {\r\n        this.detecting = false;\r\n      }\r\n    },\r\n\r\n    fileToBase64(file) {\r\n      return new Promise((resolve, reject) => {\r\n        const reader = new FileReader();\r\n        reader.onload = () => {\r\n          const result = reader.result;\r\n          if (result && result.startsWith(\"data:\")) {\r\n            resolve(result.split(\",\")[1]);\r\n          } else {\r\n            reject(new Error(\"Invalid file data\"));\r\n          }\r\n        };\r\n        reader.onerror = reject;\r\n        reader.readAsDataURL(file);\r\n      });\r\n    },\r\n\r\n    async animateScore(target) {\r\n      return new Promise((resolve) => {\r\n        const step = (target - this.score) / 30;\r\n        let count = 0;\r\n        const interval = setInterval(() => {\r\n          this.score = Math.min(100, Math.max(0, this.score + step));\r\n          count++;\r\n          if (count >= 30) {\r\n            this.score = Math.round(target);\r\n            clearInterval(interval);\r\n            resolve();\r\n          }\r\n        }, 40);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$bg: #0a0a0a;\r\n$border: rgba(255, 255, 255, 0.08);\r\n$text: #e6e8eb;\r\n\r\n.detect {\r\n  min-height: 100vh;\r\n  background: $bg;\r\n  color: $text;\r\n}\r\n\r\n.panel-wrap {\r\n  padding-left: 72px;\r\n}\r\n\r\n.fixed-panel {\r\n  posi