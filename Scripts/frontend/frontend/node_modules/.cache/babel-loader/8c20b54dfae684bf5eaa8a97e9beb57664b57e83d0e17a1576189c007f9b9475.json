urn;\n      }\n      this.detecting = true;\n      this.score = 0;\n      try {\n        const base64Audio = await this.fileToBase64(this.file);\n        const token = localStorage.getItem(\"token\");\n        console.log(\"Uploading file:\", this.fileName);\n        const response = await fetch(\"http://127.0.0.1:5000/detect\", {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: `Bearer ${token}`\n          },\n          body: JSON.stringify({\n            audio: base64Audio\n          })\n        });\n        if (!response.ok) throw new Error(`Server error: ${response.status}`);\n        const data = await response.json();\n        const targetScore = data.score ?? 0;\n        console.log(\"Detection score:\", targetScore);\n        await this.animateScore(targetScore);\n      } catch (err) {\n        console.error(\"Detection error:\", err);\n        alert(\"Error detecting watermark. Check console for details.\");\n        this.score = 0;\n      } finally {\n        this.detecting = false;\n      }\n    },\n    fileToBase64(file) {\n      return new Promise((resolve, reject) => {\n        const reader = new FileReader();\n        reader.onload = () => {\n          const result = reader.result;\n          if (result && result.startsWith(\"data:\")) {\n            resolve(result.split(\",\")[1]);\n          } else {\n            reject(new Error(\"Invalid file data\"));\n          }\n        };\n        reader.onerror = reject;\n        reader.readAsDataURL(file);\n      });\n    },\n    async animateScore(target) {\n      return new Promise(resolve => {\n        const step = (target - this.score) / 30;\n        let count = 0;\n        const interval = setInterval(() => {\n          this.score = Math.min(100, Math.max(0, this.score + step));\n          count++;\n          if (count >= 30) {\n            this.score = Math.round(target);\n            clearInterval(interval);\n         