 this.audios = [{\n          name: \"Unwatermarked\",\n          src: toBlob(res.data.unwatermarked),\n          playing: false\n        }, {\n          name: \"Watermarked\",\n          src: toBlob(res.data.watermarked),\n          playing: false\n        }];\n        this.audiosVisible = true;\n      } catch (err) {\n        console.error(\"Error generating audio:\", err);\n        alert(\"Error generating audio. Check console for details.\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    toggle(i) {\n      const ref = this.$refs[\"audio\" + i];\n      const el = Array.isArray(ref) ? ref[0] : ref;\n      const item = this.audios[i];\n      if (!el) return;\n      if (item.playing) {\n        el.pause();\n        item.playing = false;\n      } else {\n        el.play();\n        item.playing = true;\n      }\n    },\n    // ✅ Function to handle download\n    downloadAudio(audio) {\n      if (!audio.src) return;\n      const link = document.createElement(\"a\");\n      link.href = audio.src;\n      link.download = `${audio.name.replace(/\\s+/g, \"_\")}.wav`;\n      document.body.appendChild(link);\n      link.click();\n      document.body.removeChild(link);\n    }\n  }\n};","map":{"version":3,"names":["_axios","_interopRequireDefault","require","_SidePanel","_default","exports","default","name","components","SidePanel","data","loading","text","audiosVisible","audios","src","playing","methods","onGenerate","trim","alert","token","localStorage","getItem","res","axios","post","headers","Authorization","toBlob","b64","URL","createObjectURL","Blob","Uint8Array","from","atob","c","charCodeAt","type","unwatermarked","watermarked","err","console","error","toggle","i","ref","$refs","el","Array","isArray","item","pause","play","downloadAudio","audio","link","document","createElement","href","download","replace","body","appendChild","click","removeChild"],"sources":["src/views/TextToSpeech.vue"],"sourcesContent":["<template>\r\n  <div class=\"page tts\">\r\n    <side-panel />\r\n\r\n    <div class=\"looper\"></div>\r\n\r\n    <section class=\"panel-wrap\">\r\n      <div class=\"panel fixed-panel\">\r\n        <header class=\"head\">\r\n          <h2 class=\"title\">Text to Speech</h2>\r\n        </header>\r\n\r\n        <p class=\"sub\">Generate and watermark audio from text input</p>\r\n\r\n        <textarea\r\n          v-model=\"text\"\r\n          placeholder=\"Type something to generate speech...\"\r\n          rows=\"5\"\r\n          class=\"tts-input\"\r\n        ></textarea>\r\n\r\n        <div class=\"button-wrap\">\r\n          <button class=\"detect-btn\" :disabled=\"loading\" @click=\"onGenerate\">\r\n            {{ loading ? \"Generating...\" : \"Generate Audio\" }}\r\n          </button>\r\n        </div>\r\n\r\n        <!-- Audio playback + download section -->\r\n        <div v-if=\"audiosVisible\" class=\"audio-section\">\r\n          <div v-for=\"(audio, i) in audios\" :key=\"i\" class=\"audio-card\">\r\n            <h3 class=\"audio-title\">{{ audio.name }}</h3>\r\n            <audio\r\n              :ref=\"'audio' + i\"\r\n              :src=\"audio.src\"\r\n              controls\r\n              preload=\"metadata\"\r\n              class=\"audio-player\"\r\n            ></audio>\r\n\r\n            <!-- Play / Pause button -->\r\n            <button class=\"play-btn\" @click=\"toggle(i)\">\r\n              {{ audio.playing ? \"Pause\" : \"Play\" }}\r\n            </button>\r\n\r\n            <!-- ✅ Download button -->\r\n            <button class=\"download-btn\" @click=\"downloadAudio(audio)\">\r\n              Download\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport SidePanel from \"@/components/SidePanel.vue\";\r\n\r\nexport default {\r\n  name: \"TextToSpeech\",\r\n  components: { SidePanel },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      text: \"\",\r\n      audiosVisible: false,\r\n      audios: [\r\n        { name: \"Unwatermarked\", src: \"\", playing: false },\r\n        { name: \"Watermarked\", src: \"\", playing: false },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    async onGenerate() {\r\n      if (!this.text.trim()) {\r\n        alert(\"Please enter some text first!\");\r\n        return;\r\n      }\r\n\r\n      this.loading = true;\r\n      try {\r\n        const token = localStorage.getItem(\"token\");\r\n        const res = await axios.post(\r\n          \"http://127.0.0.1:5000/generate\",\r\n          { text: this.text },\r\n          {\r\n            headers: {\r\n              \"Content-Type\": \"application/json\",\r\n              Authorization: `Bearer ${token}`,\r\n            },\r\n          }\r\n        );\r\n\r\n        const toBlob = (b64) =>\r\n          URL.createObjectURL(\r\n            new Blob([Uint8Array.from(atob(b64), (c) => c.charCodeAt(0))], {\r\n              type: \"audio/wav\",\r\n            })\r\n          );\r\n\r\n        this.audios = [\r\n          { name: \"Unwatermarked\", src: toBlob(res.data.unwatermarked), playing: false },\r\n          { name: \"Watermarked\", src: toBlob(res.data.watermarked), playing: false },\r\n        ];\r\n        this.audiosVisible = true;\r\n      } catch (err) {\r\n        console.error(\"Error generating audio:\", err);\r\n        alert(\"Error generating audio. Check console for details.\");\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    toggle(i) {\r\n      const ref = this.$refs[\"audio\" + i];\r\n      const el = Array.isArray(ref) ? ref[0] : ref;\r\n      const item = this.audios[i];\r\n      if (!el) return;\r\n\r\n      if (item.playing) {\r\n        el.pause();\r\n        item.playing = false;\r\n      } else {\r\n        el.play();\r\n        item.playing = true;\r\n      }\r\n    },\r\n\r\n    // ✅ Function to handle download\r\n    downloadAudio(audio) {\r\n      if (!audio.src) return;\r\n      const link = document.createElement(\"a\");\r\n      link.href = audio.src;\r\n      link.download = `${audio.name.replace(/\\s+/g, \"_\")}.wav`;\r\n      document.body.appendChild(link);\r\n      link.click();\r\n      document.body.removeChild(link);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.page.tts {\r\n  min-height: 100vh;\r\n  background: #0a0a0a;\r\n  color: #fff;\r\n}\r\n\r\n.panel-wrap {\r\n  padding-left: 72px;\r\n}\r\n\r\n.fixed-panel {\r\n  position: relative;\r\n  left: 0;\r\n}\r\n\r\n.looper {\r\n  position: fixed;\r\n  inset: 0 0 0 72px;\r\n  background: url(~@/assets/looper-bg.png) center / 1600px auto no-repeat;\r\n  background-size: 100%;\r\n  background-position: 0px -300px;\r\n  opacity: 0.5;\r\n  transform: rotate(15deg);\r\n  pointer-events: none;\r\n}\r\n\r\n.panel {\r\n  max-width: 1077px;\r\n  margin: 0 auto 80px;\r\n  background: #000;\r\n  border: 1px solid rgba(255, 255, 255, 0.08);\r\n  border-radius: 16px;\r\n  box-shadow: 0 4px 50px rgba(33, 33, 33, 0.08), 0 4px 6px rgba(33, 33, 33, 0.04);\r\n  padding: 24px 24px 32px;\r\n  top: 160px;\r\n}\r\n\r\n.title {\r\n  font: 800 36px/1.2 \"Cabin\", sans-serif;\r\n  color: #fff;\r\n}\r\n\r\n.sub {\r\n  font: 500 18px/1.6 \"Cabin\", sans-serif;\r\n  opacity: 0.95;\r\n}\r\n\r\n.tts-input {\r\n  width: 100%;\r\n  background: #111;\r\n  border: 1px solid #333;\r\n  color: #fff;\r\n  border-radius: 8px;\r\n  padding: 12px;\r\n  font-size: 16px;\r\n  resize: none;\r\n  margin-top: 12px;\r\n}\r\n\r\n.tts-input:focus {\r\n  border-color: #3b82f6;\r\n  outline: none;\r\n}\r\n\r\n.audio-section {\r\n  margin-top: 40px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 30px;\r\n}\r\n\r\n.audio-card {\r\n  background: #111;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  border: 1px solid #333;\r\n  text-align: center;\r\n}\r\n\r\n.audio-title {\r\n  font-size: 18px;\r\n  font-weight: 700;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.audio-player {\r\n  width: 100%;\r\n  margin-bottom: 12px;\r\n}\r\n\r\n.play-btn,\r\n.download-btn {\r\n  padding: 10px 18px;\r\n  border: none;\r\n  border-radius: 6px;\r\n  color: #fff;\r\n  font-weight: 600;\r\n  cursor: pointer;\r\n  transition: 0.3s;\r\n  margin: 6px;\r\n}\r\n\r\n.play-btn {\r\n  background: #3b82f6;\r\n}\r\n.play-btn:hover {\r\n  background: #2563eb;\r\n}\r\n\r\n.download-btn {\r\n  background: #a855f7;\r\n}\r\n.download-btn:hover {\r\n  background: #9333ea;\r\n}\r\n\r\n.detect-btn {\r\n  padding: 12px 28px;\r\n  border: none;\r\n  border-radius: 10px;\r\n  background-color: #a855f7;\r\n  color: #fff;\r\n  font: 700 18px/1 \"Roboto\", sans-serif;\r\n  cursor: pointer;\r\n  transition: transform 0.2s, opacity 0.2s;\r\n  margin-top: 20px;\r\n}\r\n\r\n.detect-btn:hover {\r\n  opacity: 0.9;\r\n  transform: scale(1.05);\r\n}\r\n\r\n.detect-btn:disabled {\r\n  opacity: 0.5;\r\n  cursor: not-allowed;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;;;;;;;AAwDA,IAAAA,MAAA,GAAAC,sBAAA,CAAAC,OAAA;AACA,IAAAC,UAAA,GAAAF,sBAAA,CAAAC,OAAA;AAAA,IAAAE,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,UAAA;IAAAC,SAAA,EAAAA;EAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,IAAA;MACAC,aAAA;MACAC,MAAA,GACA;QAAAP,IAAA;QAAAQ,GAAA;QAAAC,OAAA;MAAA,GACA;QAAAT,IAAA;QAAAQ,GAAA;QAAAC,OAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACA,MAAAC,WAAA;MACA,UAAAN,IAAA,CAAAO,IAAA;QACAC,KAAA;QACA;MACA;MAEA,KAAAT,OAAA;MACA;QACA,MAAAU,KAAA,GAAAC,YAAA,CAAAC,OAAA;QACA,MAAAC,GAAA,SAAAC,cAAA,CAAAC,IAAA,CACA,kCACA;UAAAd,IAAA,OAAAA;QAAA,GACA;UACAe,OAAA;YACA;YACAC,aAAA,YAAAP,KAAA;UACA;QACA,CACA;QAEA,MAAAQ,MAAA,GAAAC,GAAA,IACAC,GAAA,CAAAC,eAAA,CACA,IAAAC,IAAA,EAAAC,UAAA,CAAAC,IAAA,CAAAC,IAAA,CAAAN,GAAA,GAAAO,CAAA,IAAAA,CAAA,CAAAC,UAAA;UACAC,IAAA;QACA,EACA;QAEA,KAAAzB,MAAA,IACA;UAAAP,IAAA;UAAAQ,GAAA,EAAAc,MAAA,CAAAL,GAAA,CAAAd,IAAA,CAAA8B,aAAA;UAAAxB,OAAA;QAAA,GACA;UAAAT,IAAA;UAAAQ,GAAA,EAAAc,MAAA,CAAAL,GAAA,CAAAd,IAAA,CAAA+B,WAAA;UAAAzB,OAAA;QAAA,EACA;QACA,KAAAH,aAAA;MACA,SAAA6B,GAAA;QACAC,OAAA,CAAAC,KAAA,4BAAAF,GAAA;QACAtB,K