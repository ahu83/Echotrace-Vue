div>\r\n            <div class=\"button-wrap\">\r\n              <button class=\"detect-btn\" @click=\"toggleDetect\">{{ buttonText }}</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidePanel from '@/components/SidePanel.vue';\r\n\r\nexport default {\r\n  name: 'DetectionResult',\r\n  components: {SidePanel},\r\n  data() {\r\n    const size = 466;\r\n    const stroke = 48;\r\n    const center = size / 2;\r\n    const radius = (size - stroke) / 2;\r\n    const circumference = 2 * Math.PI * radius;\r\n    return {\r\n      size, stroke, center, radius, circumference,\r\n      gradId: 'grad-donut-' + Math.random().toString(36).slice(2, 8),\r\n      rotate: `rotate(-90 ${center} ${center})`,\r\n      score: 0,\r\n      detecting: false,\r\n      intervalId: null\r\n    };\r\n  },\r\n  computed: {\r\n    buttonText() {\r\n      return this.detecting ? 'Detecting...' : 'Detect';\r\n    }\r\n  },\r\n  methods: {\r\n    async toggleDetect() {\r\n      if (this.detecting) return;\r\n      this.detecting = true;\r\n      this.score = 0;\r\n      await fetch(\"http://localhost:5000/start\", { method: \"POST\" });\r\n      this.intervalId = setInterval(async () => {\r\n        const res = await fetch(\"http://localhost:5000/progress\");\r\n        const data = await res.json();\r\n        this.score = data.value;\r\n        if (data.value >= 100) {\r\n          clearInterval(this.intervalId);\r\n          this.detecting = false;\r\n        }\r\n      }, 100);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$bg: #0A0A0A;\r\n$panel: #000;\r\n$border: rgba(255, 255, 255, .08);\r\n$text: #E6E8EB;\r\n\r\n.detect {\r\n  min-height: 100vh;\r\n  background: $bg;\r\n  color: $text;\r\n  position: relative;\r\n}\r\n\r\n.looper {\r\n  position: fixed;\r\n  inset: 0 0 0 72px;\r\n  background: url(~@/assets/looper-bg.png) center / 1600px auto no-repeat;\r\n 